generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum Role{
    ADMIN
    CUSTOMER
    COLLABORATOR
}

model User{
    id String @id @unique @default(uuid())
    email String @unique
    name String
    password String
    role Role @default(CUSTOMER)
}